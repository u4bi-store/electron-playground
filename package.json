{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . u4bi-app",
    "package": "asar pack u4bi-app-darwin-x64/u4bi-app.app/Contents/Resources/app u4bi-app-darwin-x64/u4bi-app.app/Contents/Resources/app.asar && rm -r u4bi-app-darwin-x64/u4bi-app.app/Contents/Resources/app"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "asar": "^0.14.5",
    "electron": "^3.0.0",
    "electron-packager": "^12.2.0"
  }
}
